/**
 * Demultiplexor:
 * {a, b} = {in, 0} if selected == 0
 *          {0, in} otherwise
 */

CHIP DMux {
    IN in, selected;
    OUT a, b;

    PARTS:
    Not(in=selected, out=notselected);
    And(a=in, b=notselected, out=a);
    Not(in=notselected, out=b);
}
